{
  "name": "janssen-ecosystem",
  "private": true,
  "version": "0.1.0",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "scripts": {
    "dev:superyachts": "pnpm -C apps/superyachts dev",
    "dev:crypto": "pnpm -C apps/crypto dev",
    "dev:institute": "pnpm -C apps/janssen-institute dev",
    "dev:all": "concurrently -n superyachts,crypto,institute -c green,cyan,magenta pnpm:\"-C apps/superyachts dev\" pnpm:\"-C apps/crypto dev\" pnpm:\"-C apps/janssen-institute dev\"",
    "build:superyachts": "pnpm -C apps/superyachts build",
    "build:crypto": "pnpm -C apps/crypto build",
    "build:institute": "pnpm -C apps/janssen-institute build"
  }
}
